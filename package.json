{
  "name": "ohauth",
  "version": "0.2.2",
  "description": "Pure-browser OAuth for CORS-supporting sites like OpenStreetMap",
  "main": "index.js",
  "repository": "osmlab/ohauth",
  "author": "Tom MacWright",
  "license": "ISC",
  "keywords": [
    "oauth",
    "browser"
  ],
  "testling": {
    "files": [
      "test/*.js"
    ],
    "harness": "mocha",
    "browsers": {
      "ie": [
        9,
        10
      ],
      "firefox": [
        13
      ],
      "chrome": [
        20
      ],
      "safari": [
        5.1
      ],
      "opera": [
        12
      ]
    }
  },
  "dependencies": {
    "jshashes": "~1.0.3",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "browserify": "~13.1.0",
    "eslint": "~3.6.0",
    "tap": "~7.1.0"
  },
  "scripts": {
    "build": "browserify index.js -s ohauth > ohauth.js",
    "lint": "eslint index.js test/",
    "test": "npm run build && npm run lint && tap --cov test/*.js"
  }
}
